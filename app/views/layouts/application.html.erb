<!DOCTYPE html>
<html>
<head>
  <title>BraintreeExamples</title>
  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>
  <%= yield :head %>
</head>
<body>
<% flash.each do |name, msg| %>
  <%= content_tag :div, msg, class: name %>
<% end %>
<div><%= link_to('home', root_path) %></div>
<%= yield %>

</body>
</html>
